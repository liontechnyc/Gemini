{
  "name": "@liontechnyc/gemini",
  "description": "An unobtrusive UI templating framework wrapping over CSS Grid/Flex optimized for minimal markup and responsive display",
  "version": "1.0.1-3",
  "author": "Ezra Singh <ezra@liontech.nyc>",
  "keywords": [
    "gatsby",
    "react",
    "typescript",
    "frontend"
  ],
  "license": "GPL-2.0-only",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rollup -c",
    "storybook": "start-storybook -p 5005",
    "storybook:export": "build-storybook",
    "format": "tslint \"{src}/**/*.{ts,tsx}\" --quiet --fix || prettier --write \"*/**/*.{js,json,md,scss}\"",
    "lint-staged": "tslint \"src/**/*.{js,ts,tsx}\" --fix || prettier --write \"*/**/*.{js,json,md,scss}\"",
    "test": "jest --silent",
    "test:watch": "jest --watch --silent",
    "publish": "storybook-to-ghpages",
    "type-check": "tsc --noEmit"
  },
  "repository": "github:liontechnyc/Gemini",
  "bugs": {
    "url": "https://liontech.nyc",
    "email": "ezra@liontech.nyc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "devDependencies": {
    "@artsy/fresnel": "^1.2.0",
    "@babel/core": "^7.10.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@storybook/react": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.18",
    "@types/react": "^16.9.41",
    "@types/styled-components": "^5.1.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.20.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "sass-loader": "^9.0.1",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@artsy/fresnel": ">=1.2.0"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "dependencies": {},
  "directories": {
    "example": "examples"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
